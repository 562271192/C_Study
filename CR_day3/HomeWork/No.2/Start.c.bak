#include <stdio.h>
#include <string.h>
/*
	* Project content：从键盘输入10个数，求其中最大数和最小数并输出
	* Creating time: 2021-06-29
	* Founder: Jingjie
*/
int main(int argc, char* argv[])
{
	//设置初始值
	int i = 0, x = 0;
	for (i = 1; i <= 9; i++)
	{
		for (x = 1; x <= i; x++)
		{
			printf("%d ",i*x);
		}

		printf("\n");
	}
	

	system("pause");

	return 0;
}
